times=0

function CriarItem()
  local result,objid=World:spawnItem(0,7,0,101,1)  
 
end  



function main()
    

if times~=5 then 
    times=times+1 
     Chat:sendSystemMsg("Times="..times,0)
    return
    end
  CriarItem() 
  times=0
  
    
 end   
ScriptSupportEvent:registerEvent([=[Game.RunTime]=],main) 
